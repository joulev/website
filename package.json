{
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "biome check .",
    "check:ci": "biome ci .",
    "build": "bun download-fonts && bun download-theme && bun codegen && next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "db:push": "drizzle-kit push",
    "deploy-worker": "wrangler deploy misc/multi-tenancy-worker.ts --name multi-tenant-on-website --compatibility-date 2023-12-05",
    "dev": "concurrently \"bun codegen --watch\" \"next dev\"",
    "download-fonts": "./scripts/download-fonts.sh",
    "download-theme": "./scripts/download-theme.sh",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit"
  },
  "packageManager": "bun@1.1.13",
